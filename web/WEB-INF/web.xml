<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>GpsServlet</servlet-name>
        <servlet-class>bash.GpsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GpsLocation</servlet-name>
        <servlet-class>bash.GpsLocation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GpsGet</servlet-name>
        <servlet-class>bash.GpsGet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GpsServlet</servlet-name>
        <url-pattern>/GpsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GpsLocation</servlet-name>
        <url-pattern>/GpsLocation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GpsGet</servlet-name>
        <url-pattern>/GpsGet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
